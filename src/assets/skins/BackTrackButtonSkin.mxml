<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark" 
			 xmlns:mx="library://ns.adobe.com/flex/mx" 
			 width="30" height="31"
			 minHeight="30" minWidth="31">
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:BitmapImage source="@Embed('assets/images/backtrackButton.png')" includeIn="up,disabled,down" alpha.disabled="0.5"/>
	<s:BitmapImage source="@Embed('assets/images/backtrackButtonOver.png')" includeIn="over" />
	
	<s:Group id="contentGroup" left="0" right="0" top="0" bottom="0" />
	
</s:SparkSkin>
