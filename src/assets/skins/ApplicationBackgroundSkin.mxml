<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark" 
			 xmlns:mx="library://ns.adobe.com/flex/mx"
			 width="100%" height="100%" 
			 minHeight="10" minWidth="10">
	
	<fx:Metadata>
		[HostComponent("spark.components.Application")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect left="0" right="0" top="50" bottom="0">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:entries>
					<s:GradientEntry color="0x000000" ratio="0" />
					<s:GradientEntry color="0x504a4a" ratio=".66" />
				</s:entries>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	
	<s:Rect top="0" left="0" right="0" height="50">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="0xFFFFFF" ratio="0"/>
				<s:GradientEntry color="0xdbdbdb" ratio=".33"/>
				<s:GradientEntry color="0x999899" ratio=".66"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" left="0" right="0" top="0" bottom="0" >
		<s:layout>
			<s:BasicLayout />
		</s:layout>
	</s:Group>
	
</s:SparkSkin>
